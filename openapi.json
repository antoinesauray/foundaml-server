      "Project": {
        "type": "object",
        "required": [
          "id",
          "name",
          "problem",
          "algorithms",
          "policy",
          "configuration"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "problem": {
            "type": "string"
          },
          "algorithms": {
            "type": "array"
          }
        }
      },
      "Algorithm": {
        "type": "object",
        "required": [
          "id",
          "projectId",
          "backend",
          "security"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "projectId": {
            "type": "string"
          },
          "backend": {
