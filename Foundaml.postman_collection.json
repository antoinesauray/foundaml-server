		{
			"name": "Create an algorithm",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n\t\"id\": \"tf-kaggle-titanic-1\",\n\t\"projectId\": \"kaggle-titanic\",\n\t\"backend\": {\n\t\t\"class\": \"TensorFlowBackend\",\n\t\t\"host\": \"127.0.0.1\",\n\t\t\"port\": 3000,\n\t\t\"featuresTransformer\": {\n\t\t\t\"signatureName\": \"\",\n\t\t\t\"fields\": [\n\t\t\t\t\"passenger_id\",\n\t\t\t\t\"p_class\",\n\t\t\t\t\"name\",\n\t\t\t\t\"sex\",\n\t\t\t\t\"age\",\n\t\t\t\t\"sib_sp\",\n\t\t\t\t\"p_arch\",\n\t\t\t\t\"ticket\",\n\t\t\t\t\"fare\",\n\t\t\t\t\"cabin\",\n\t\t\t\t\"embarked\"\n\t\t\t]\n\t\t},\n\t\t\"labelsTransformer\": {\n\t\t\t\"fields\": {\n\t\t\t\t\"survived\": \"survived\",\n\t\t\t\t\"did_not_survived\": \"notSurvived\"\n\t\t\t}\n\t\t}\n\t}\n}"
				},
				"url": {
					"raw": "http://localhost:8080/algorithms/",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8080",
